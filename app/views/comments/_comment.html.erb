<div class="row comment-fix">
  <div class="col-md-2">
  </div>
  <div class="col-md-8 well" id="comment_well">
    <strong style="display: inline"><%= comment.user.username %></strong>:
    <p style="display: inline"><%= comment.description %></p>
    <p>
      <span class="quiet"><small>Created: <%= time_ago_in_words(comment.created_at) %>
      ago by <%= comment.user.username %></small></span>
    </p>
  </div>
</div>